{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "PHASE=production node dist/index.js",
    "dev": "PHASE=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ --fix",
    "test": "PHASE=test jest --config jest.config.mjs --detectOpenHandles",
    "test:watch": "PHASE=test jest --config jest.config.mjs --detectOpenHandles --watch",
    "test:e2e": "PHASE=test jest --config jest.e2e.config.mjs --detectOpenHandles",
    "test:e2e:watch": "PHASE=test jest --config jest.e2e.config.mjs --detectOpenHandles --watch"
  },
  "dependencies": {
    "@repo/logger": "workspace:*",
    "@repo/entities": "workspace:*",
    "@types/lodash": "^4.17.14",
    "axios": "^1.8.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.18.3",
    "express-session": "^1.18.1",
    "google-auth-library": "^9.15.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "minio": "^8.0.6",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.13.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.20",
    "web-push": "^3.6.7",
    "bullmq": "^5.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/constants": "workspace:*",
    "@repo/interfaces": "workspace:*",
    "@repo/jest-presets": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/supertest": "^6.0.2",
    "@types/web-push": "^3.6.4",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.2",
    "typescript": "5.5.4"
  }
}
