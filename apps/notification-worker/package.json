{
  "name": "notification-worker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "PHASE=production node dist/index.js",
    "dev": "PHASE=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ --fix",
    "test": "PHASE=test jest --config jest.config.mjs --detectOpenHandles",
    "test:watch": "PHASE=test jest --config jest.config.mjs --detectOpenHandles --watch",
    "test:e2e": "PHASE=test jest --config jest.e2e.config.mjs --detectOpenHandles",
    "test:e2e:watch": "PHASE=test jest --config jest.e2e.config.mjs --detectOpenHandles --watch"
  },
  "dependencies": {
    "@repo/logger": "workspace:*",
    "@repo/entities": "workspace:*",
    "@repo/notification": "workspace:*",
    "bullmq": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^9.0.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "lodash": "^4.17.21",
    "nodemailer": "^6.9.16",
    "pg": "^8.13.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.20",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/interfaces": "workspace:*",
    "@repo/jest-presets": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/lodash": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/nodemailer": "^6.4.17",
    "@types/web-push": "^3.6.4",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.2",
    "typescript": "5.5.4"
  }
}
